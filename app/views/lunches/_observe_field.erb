<%= observe_field field_name, :url => { 
  :action => 'show_by_date_or_dateless' }, 
  :with => "'year=' + $('date_year').getValue() + 
            '&month=' + $('date_month').getValue() + 
            '&day=' + $('date_day').getValue() +
            '&include_dateless=' + $(include_datelss).getValue()" %>
